{
  "name": "imagina/media-module",
  "type": "asgard-module",
  "description": "Media module for AsgardCMS. Handles the media library.",
  "keywords": [
    "asgardcms",
    "media",
    "files",
    "library",
    "thumbnails",
    "filters"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Widart",
      "email": "info@asgardcms.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@asgardcms.com",
    "issues": "https://github.com/AsgardCms/Platform/issues",
    "source": "https://github.com/AsgardCms/Media"
  },
  "require": {
    "php": "^8.1",
    "composer/installers": "~1.0",
    "intervention/image": "^2.5",
    "imagina/tag-module": "^10.0",
    "guzzlehttp/psr7": "^2.6"
    
  },
  "require-dev": {
    "orchestra/testbench": "^8.5",
    "phpunit/phpunit": "^10",
    "league/flysystem-aws-s3-v3": "^3.0",
    "mockery/mockery": "^1.4.4",
    "friendsofphp/php-cs-fixer": "^3.27.0",
    "doctrine/dbal": "^3.5"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Media\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.0.x-dev",
      "dev-v10.x": "10.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
